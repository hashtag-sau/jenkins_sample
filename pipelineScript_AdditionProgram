pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git branch: 'main', url: 'https://github.com/hashtag-sau/jenkins_sample.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x Addition.py"
                sh "./Addition.py"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x Test.py"
                sh "./Test.py"
            }
        }
    } 
}

